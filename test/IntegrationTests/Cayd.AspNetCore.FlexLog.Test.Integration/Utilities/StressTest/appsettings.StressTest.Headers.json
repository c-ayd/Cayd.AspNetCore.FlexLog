{
  "FlexLog": {
    "BufferLimit": 10000,
    "TimerInSeconds": 300,
    "LogDetails": {
      "Claims": {
        "Enabled": false
      },
      "Headers": {
        "CorrelationIdKey": "Correlation-Id",
        "IgnoredKeys": [
          "Connection"
        ]
      },
      "RequestBody": {
        "Enabled": false
      },
      "ResponseBody": {
        "Enabled": false
      },
      "QueryString": {
        "Enabled": false
      }
    }
  }
}