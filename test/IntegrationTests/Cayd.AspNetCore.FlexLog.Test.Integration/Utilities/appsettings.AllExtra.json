{
  "FlexLog": {
    "Channel": {
      "Strategy": "Unbounded",
      "Capacity": 1000
    },
    "BufferSize": 100,
    "TimerInSeconds": 3,
    "IgnoredRoutes": [
      "/ignore"
    ],
    "LogDetails": {
      "Claims": {
        "Enabled": true,
        "IncludedTypes": [
          "NameIdentifier"
        ],
        "IgnoredTypes": [
          "NameIdentifier",
          "Email"
        ]
      },
      "Headers": {
        "Enabled": true,
        "IncludedKeys": [
          "User-Agent"
        ],
        "IgnoredKeys": [
          "User-Agent",
          "Connection"
        ]
      },
      "RequestBody": {
        "Enabled": true,
        "BodySizeLimitInBytes": 50000,
        "RedactedKeys": [
          "password"
        ]
      },
      "ResponseBody": {
        "Enabled": true,
        "RedactedKeys": [
          "secret"
        ]
      }
    }
  }
}